{
  "name": "@neovici/cosmoz-bottom-bar",
  "version": "11.0.1",
  "description": "A responsive bottom-bar that can house buttons/actions and a menu for the buttons that won't fit the available width.",
  "keywords": [
    "polymer",
    "web-components"
  ],
  "homepage": "https://github.com/Neovici/cosmoz-bottom-bar#readme",
  "bugs": {
    "url": "https://github.com/Neovici/cosmoz-bottom-bar/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neovici/cosmoz-bottom-bar.git"
  },
  "license": "Apache-2.0",
  "author": "Neovici Development",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist/*",
    "types"
  ],
  "scripts": {
    "lint": "tsc && eslint --cache .",
    "build": "tsc -p tsconfig.build.json",
    "check:duplicates": "check-duplicate-components",
    "start": "npm run storybook:start",
    "test": "wtr --coverage",
    "test:watch": "wtr --watch",
    "prepare": "husky",
    "storybook:start": "storybook dev -p 8000",
    "storybook:build": "storybook build",
    "storybook:deploy": "storybook-to-ghpages",
    "storybook:preview": "npm run storybook:build && http-server -d ./storybook-static/"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@neovici/cosmoz-collapse": "^1.5.0",
    "@neovici/cosmoz-dropdown": "^7.0.0",
    "@neovici/cosmoz-utils": "^6.13.1",
    "@pionjs/pion": "^2.5.2",
    "@polymer/polymer": "^3.3.0",
    "lit-html": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@neovici/cfg": "^2.8.0",
    "@open-wc/testing": "^4.0.0",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/paper-button": "^3.0.0",
    "@polymer/paper-toggle-button": "^3.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@storybook/web-components-vite": "^10.1.11",
    "@types/mocha": "^10.0.6",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "esbuild": "^0.27.0",
    "eslint": "^9.0.0",
    "eslint-plugin-storybook": "10.2.8",
    "http-server": "^14.1.1",
    "husky": "^9.1.1",
    "lint-staged": "^16.2.7",
    "rollup-plugin-esbuild": "^6.1.1",
    "semantic-release": "^25.0.1",
    "sinon": "^21.0.0",
    "storybook": "^10.1.11",
    "typescript": "^5.0.0"
  }
}
