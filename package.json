{
	"name": "@neovici/cosmoz-bottom-bar",
	"version": "4.0.5",
	"description": "A responsive bottom-bar that can house buttons/actions and a menu for the buttons that won't fit the available width.",
	"keywords": [
		"polymer",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-bottom-bar#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-bottom-bar/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-bottom-bar.git"
	},
	"license": "Apache-2.0",
	"author": "Neovici Development",
	"main": "cosmoz-bottom-bar.js",
	"directories": {
		"test": "test"
	},
	"files": [
		"cosmoz-*.js",
		"lib/**/*.js"
	],
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"start": "es-dev-server",
		"test": "karma start --coverage",
		"test:watch": "karma start --auto-watch=true --single-run=false",
		"test:update-snapshots": "karma start --update-snapshots",
		"test:prune-snapshots": "karma start --prune-snapshots",
		"test:compatibility": "karma start --compatibility all --auto-watch=true --single-run=false"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master"
	},
	"publishConfig": {
		"access": "public"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"dependencies": {
		"@neovici/cosmoz-utils": "^3.19.0",
		"@neovici/cosmoz-viewinfo": "^3.1.3",
		"@polymer/iron-icons": "^3.0.1",
		"@polymer/iron-selector": "^3.0.0",
		"@polymer/paper-icon-button": "^3.0.0",
		"@polymer/paper-listbox": "^3.0.0",
		"@polymer/paper-menu-button": "^3.1.0",
		"@polymer/polymer": "^3.3.1",
		"@webcomponents/shadycss": "^1.10.2",
		"haunted": "^4.8.0",
		"lit-html": "^1.4.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@neovici/eslint-config": "^1.3.2",
		"@open-wc/testing": "^2.5.33",
		"@open-wc/testing-karma": "^3.3.12",
		"@polymer/iron-icon": "^3.0.1",
		"@polymer/paper-button": "^3.0.0",
		"@polymer/paper-toggle-button": "^3.0.0",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"@webcomponents/webcomponentsjs": "^2.5.0",
		"deepmerge": "^4.2.2",
		"es-dev-server": "^1.60.2",
		"eslint": "^6.8.0",
		"husky": "^4.3.8",
		"karma": "^5.0.1",
		"karma-firefox-launcher": "^1.3.0",
		"karma-sauce-launcher": "^4.3.6",
		"semantic-release": "^17.4.3",
		"sinon": "^9.2.4"
	}
}
